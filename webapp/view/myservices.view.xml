<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="inc.demo.Toyota.controller.myservices"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page class="homePage">
		<customHeader>
			<Bar class="stylebar">
				<contentMiddle>
					<Label text="MY SERVICES" class="logo"></Label>
				</contentMiddle>
				<contentRight>
					<Button icon="sap-icon://decline" class="btn" press="onClose"></Button>
				</contentRight>
			</Bar>
		</customHeader>
		<content>
			<Label design="Bold" text="Service Type" class="sapUiSmallMarginBegin sapUiSmallMarginTop heading"></Label>
			<HBox class="sapUiMediumMarginTop">
				<Button id="ongoing" text="Ongoing" class="sapUiMediumMarginBegin sapUiTinyMarginEnd" press="onOngoingPress"></Button>
				<Button id="past" text="Past" class="sapUiTinyMarginEnd" press="onPastPress"></Button>
				<Button id="upcoming" text="Upcoming" press="onUpcomingPress"></Button>
			</HBox>
			<VBox class="sapUiSmallMarginTop myServicesVbox">
				<Label design="Bold" text="Services" class="sapUiSmallMarginBegin sapUiSmallMarginTop heading"></Label>
				<VBox class="sapUiMediumMarginBegin sapUiSmallMarginTop myServicesHbox">
					<HBox>
						<VBox>
							<Image src="./Images/Artboard2.png" class="myservicesimg"></Image>
							<Text text="VIN:LS81234" class="myservicesText1"></Text>
							<Text text="Brake Service" class="sapUiTinyMarginTop myservicesText1"></Text>
						</VBox>
						<Button text="Know More" class="myserviceKnowmoreBtn"></Button>
					</HBox>
					<HBox class="sapUiTinyMarginTop">
						<Image src="./Images/group3.png"></Image>
						<Text text="30 May 2020, 09:00 AM - 10:00 AM" class="sapUiTinyMarginTop sapUiTinyMarginBegin"></Text>
					</HBox>
				</VBox>
				<VBox class="sapUiMediumMarginBegin sapUiSmallMarginTop myServicesHbox">
					<HBox>
						<VBox>
							<Image src="./Images/Artboard2.png" class="myservicesimg"></Image>
							<Text text="VIN:RSS81234" class="myservicesText1"></Text>
							<Text text="Brake Service" class="sapUiTinyMarginTop myservicesText1"></Text>
						</VBox>
						<Button text="Know More" class="myserviceKnowmoreBtn"></Button>
					</HBox>
					<HBox class="sapUiTinyMarginTop">
						<Image src="./Images/group3.png"></Image>
						<Text text="30 May 2020, 09:00 AM - 10:00 AM" class="sapUiTinyMarginTop sapUiTinyMarginBegin"></Text>
					</HBox>
				</VBox>
			</VBox>
		</content>
	</Page>
</mvc:View>