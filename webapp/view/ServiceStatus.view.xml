												stroke-miterlimit="10" stroke-width="1.44"/>
											<path id="Path_18" data-name="Path 18" d="M30.24,87.84,46.08,61.92,27.36,5.76,0,0Z" transform="translate(0 57.6)" fill="{oDataGlobalModel>/serviceStatusView/fill4}"
												stroke="#979797" stroke-miterlimit="10" stroke-width="1.44"/>
											<path id="Path_19" data-name="Path 19" d="M76.2,0V25.92L27.24,63.36,0,57.56Z" transform="translate(0.12)" fill="{oDataGlobalModel>/serviceStatusView/fill5}" stroke="#979797"
												stroke-miterlimit="10" stroke-width="1.44"/>
										</g>
									</g>
									<text id="Waiting_for_approval" data-name="Waiting for approval" transform="translate(84 74)" fill="#fff" font-size="12"
										font-family="Karbon-Semibold, Karbon" font-weight="600" letter-spacing="0.125em">
										<tspan x="-35.736" y="0">Waiting for</tspan>
										<tspan y="0" font-family="LucidaGrande-Bold, Lucida Grande" font-weight="700"></tspan>
										<tspan x="-26.79" y="14">approval</tspan>
									</text>
								</g>
							</g>
							<g id="Group_7" data-name="Group 7" transform="translate(-52)">
								<g id="Group_6" data-name="Group 6">
									<text id="Order_created" data-name="Order created" transform="translate(170 10)" fill="#00d1ce" font-size="14"
										font-family="Karbon-Semibold, Karbon" font-weight="600" letter-spacing="0.125em">
										<tspan x="0" y="0">Order created</tspan>
									</text>
									<text id="Car_received" data-name="Car received" transform="translate(300 84)" fill="#00d1ce" font-size="16"
										font-family="Karbon-Semibold, Karbon" font-weight="600" letter-spacing="0.125em">
										<tspan x="0" y="0">Car received</tspan>
									</text>
									<text id="Quality_Inspection" data-name="Quality Inspection" transform="translate(112.5 161)" font-size="16"
										font-family="Karbon-Semibold, Karbon" font-weight="600" letter-spacing="0.125em">
										<tspan x="-33.592" y="0">Quality
										</tspan>
										<tspan y="0" font-family="LucidaGrande-Bold, Lucida Grande" font-weight="700"></tspan>
										<tspan x="-43.632" y="19">Inspection</tspan>
									</text>
									<text id="Ready_for_Delivery" data-name="Ready for Delivery" transform="translate(92 84)" font-size="16"
										font-family="Karbon-Semibold, Karbon" font-weight="600" letter-spacing="0.125em">
										<tspan x="-40.144" y="0">Ready for</tspan>
										<tspan y="0" font-family="LucidaGrande-Bold, Lucida Grande" font-weight="700"></tspan>
										<tspan x="-34.464" y="19">Delivery</tspan>
									</text>
									<text id="Service_done-2" data-name="Service done" transform="translate(276 171)" fill="#00d1ce" font-size="16"
										font-family="Karbon-Semibold, Karbon" font-weight="600" letter-spacing="0.125em">
										<tspan x="0" y="0">Service done</tspan>
									</text>
								</g>
							</g>
						</g>
					</svg>
				</VBox>
			</VBox>
			<HBox justifyContent="Center" alignContent="Center">
				<List>
					<CustomListItem class="aljListLi">
						<HBox alignItems="Center">
							<Image src="Images/carUs.png"></Image>
							<VBox class="sapUiSmallMarginBegin sapUiSmallMarginEnd">
								<Text text="{oDataGlobalModel>/CarServiceStatus/carName}" class="aljHeadingText"></Text>
							</VBox>
							<Text text="IN" class="aljBoldText sapUiTinyMarginEnd"></Text>
							<Switch class="aljSwitch" state="false" customTextOn=" " customTextOff=" ">
								<!--<layoutData>-->
								<!--	<FlexItemData growFactor="1"/>-->
								<!--</layoutData>-->
							</Switch>
							<Text text="OUT" class="aljBoldText sapUiTinyMarginBegin"></Text>
						</HBox>
					</CustomListItem>
					<CustomListItem class="aljListLi">
						<HBox>
							<Image src="Images/icons8-tow-truck-64-6.png"></Image>
							<VBox class="sapUiSmallMarginBegin sapUiSmallMarginEnd">
								<Text text="{oDataGlobalModel>/CarServiceStatus/Date}" class="aljHeadingText"></Text>
							</VBox>
						</HBox>
					</CustomListItem>
					<CustomListItem class="aljListLi">
						<HBox>
							<Image src="Images/icons8-tow-truck-64-6.png"></Image>
							<VBox class="sapUiSmallMarginBegin sapUiSmallMarginEnd">
								<Text text="{oDataGlobalModel>/CarServiceStatus/Location}" class="aljHeadingText"></Text>
							</VBox>
							<Image src="Images/mapIcon.png" press="onMapOpen" width="1.3rem"></Image>
						</HBox>
					</CustomListItem>
					<CustomListItem class="aljListLi">
						<HBox>
							<Image src="Images/icons8-tow-truck-64-6.png"></Image>
							<VBox class="sapUiSmallMarginBegin sapUiSmallMarginEnd">
								<Text text="Mohammad Ahmed,(Security Advisor)" class="aljHeadingText"></Text>
							</VBox>
						</HBox>
					</CustomListItem>
					<CustomListItem class="aljListLi">
						<HBox>
							<Image src="Images/icons8-tow-truck-64-6.png"></Image>
							<VBox class="sapUiSmallMarginBegin sapUiSmallMarginEnd">
								<Text text="987-65432-8735" class="aljHeadingText"></Text>
							</VBox>
							<Image src="Images/Call.svg" press="onCall"  width="1.3rem"></Image>
						</HBox>
					</CustomListItem>
				</List>
			</HBox>
			<HBox class="sapUiSmallMarginTop sapUiMediumMarginBottom" justifyContent="Center" alignContent="Center">
				<VBox justifyContent="Center" alignContent="Center">
					<Button class="aljRedButton aljGrayButtons" text="Copy to mobile calendar"></Button>
					<Button class="aljRedButton aljGrayButtons" text="Reschedule" press="onReschedulePress"></Button>
					<Button class="aljRedButton aljGrayButtons" text="Cancel Appointment" press="onCancelAppointmentPress"></Button>
				</VBox>
			</HBox>
		</content>
		<HBox class="aljGrayHbox sapUiSmallMarginTop" justifyContent="Center" alignContent="Center">
			<VBox class="sapUiSmallMarginTop sapUiMediumMarginBottom" width="85%">
				<HBox>
					<Label required="true" class="sapUiTinyMarginBottom sapUiSmallMarginTop aljBlackStar aljRedUnderline" text="Price Breakup"></Label>
				</HBox>
				<VBox class="aljWhiteSmokeVbox sapUiTinyMarginTop">
					<Panel expandable="true" width="auto" class="sapUiResponsiveMargin ">
						<headerToolbar>
							<OverflowToolbar style="Clear">
								<Title text="80,000 KM Service"/>
								<ToolbarSpacer/>
								<Text class="sapUiSmallMarginEnd" text="SAR 100"></Text>
							</OverflowToolbar>
						</headerToolbar>
						<content>
							<Text text="qqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqq"></Text>
						</content>
					</Panel>
					<Panel expandable="true" width="auto" class="sapUiResponsiveMargin">
						<headerToolbar>
							<OverflowToolbar style="Clear">
								<Title text="Brake Service"/>
								<ToolbarSpacer/>
								<Text class="sapUiSmallMarginEnd" text="SAR 100"></Text>
							</OverflowToolbar>
						</headerToolbar>
						<content>
							<Text text="qqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqq"></Text>
						</content>
					</Panel>
					<Panel expandable="true" width="auto" class="sapUiResponsiveMargin">
						<headerToolbar>
							<OverflowToolbar style="Clear">
								<Label required="true" text="AC Service"/>
								<ToolbarSpacer/>
								<Text class="sapUiSmallMarginEnd" text="SAR 100"></Text>
							</OverflowToolbar>
						</headerToolbar>
						<content>
							<Text
								text="qqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrrr"></Text>
						</content>
					</Panel>
					<Bar class="aljPadding">
						<contentLeft class="aljPadding">
							<Text class="aljBoldText" text="Total Price"></Text>
						</contentLeft>
						<contentRight class="aljPadding">
							<Text class="aljBoldText" text="SAR 300"></Text>
						</contentRight>
					</Bar>
					<Bar class="aljPadding">
						<contentLeft class="aljPadding">
							<Text class="aljBoldText" text="Payment Status"></Text>
						</contentLeft>
						<contentRight class="aljPadding">
							<Text class="aljBoldText" text="Pending"></Text>
						</contentRight>
					</Bar>
					<HBox justifyContent="End">
						<Label required="true" class="sapUiSmallMarginEnd sapUiTinyMarginBottom aljRedLabel sapUiSmallMarginTop aljRedText" text="Additional Job"></Label>
					</HBox>
				</VBox>
			</VBox>
		</HBox>
	</Page>
</mvc:View>