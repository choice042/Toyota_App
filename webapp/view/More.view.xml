<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="inc.demo.Toyota.controller.More"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:l="sap.ui.layout">
	<App>
		<pages>
			<Page title="More">
				<content>
					<VBox>
						<HBox>
							<VBox class="productsClass sapUiSmallMarginBegin sapUiTinyMarginTop sapUiTinyMarginBottom sapUiTinyMarginEnd" justifyContent="Center">
								<!--<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Safety and Maintenance Tips" -->
								<!--	press="press">-->
								
								<!--</GenericTile>-->
									<Label text="Safety and Maintenance Tips" wrapping="true" design="Bold" class="labelclass"></Label>
							</VBox>
							<VBox class="productsClass sapUiTinyMarginBegin sapUiTinyMarginTop sapUiTinyMarginBottom sapUiTinyMarginEnd">
								<Label text="Default Service Centre" wrapping="true" design="Bold" class="labelclass"></Label>
									<Label text="Medinah Road" wrapping="true" design="Bold" class="labelclass"></Label>
							</VBox>
						</HBox>
						<VBox class="productsClass sapUiSmallMarginBegin sapUiTinyMarginTop sapUiTinyMarginBottom sapUiTinyMarginEnd">
							<Image src="./Images/Artboard3.png"></Image>
							<Label text="Battery" design="Bold" class="labelclass"></Label>
							<Image src="./Images/product2.png"></Image>
							<Label text="SAR100" design="Bold" class="labelclass2"></Label>
							<Text></Text>
							<Button text="Book" class="productsbuttonclass"></Button>
						</VBox>
					</VBox>
				</content>
				<footer>
					<Toolbar class="stylefooter">
						<VBox>
							<VBox class="text1">
								<Button icon="./Images/icons8-home-16-2.png" press="onHomePress" tooltip="Home" class="footerbtn"></Button>
							</VBox>
							<VBox class="text2">
								<Label id="home" text="Home" design="Bold"></Label>
							</VBox>
						</VBox>
						<ToolbarSpacer/>
						<VBox>
							<VBox class="text1">
								<Button icon="./Images/MyCar.svg" press="onMyCarPress" tooltip="rental" class="footerbtn"></Button>
							</VBox>
							<VBox class="text2">
								<Label id="mycar" text="My Car" design="Bold"></Label>
							</VBox>
						</VBox>
						<ToolbarSpacer/>
						<VBox>
							<VBox class="centralbutton">
								<Button icon="./Images/products.png" press="onProductPress" class="productsfooterbtn"></Button>
							</VBox>
							<VBox class="text3">
								<Label id="products" text="Products" design="Bold"></Label>
							</VBox>
						</VBox>
						<ToolbarSpacer/>
						<VBox>
							<VBox class="text1">
								<Button icon="./Images/calendar.png" press="onBookservicePress" class="footerbtn"></Button>
							</VBox>
							<VBox class="text2">
								<Label id="bookings" text="Bookings" design="Bold"></Label>
							</VBox>
						</VBox>
						<ToolbarSpacer/>
						<VBox>
							<VBox class="text1">
								<Button icon="./Images/More-4.png" press="onMorePress" class="footerbtn"></Button>
							</VBox>
							<VBox class="text2">
								<Label id="more" text="More" design="Bold"></Label>
							</VBox>
						</VBox>
					</Toolbar>
				</footer>
			</Page>
		</pages>
	</App>
</mvc:View>