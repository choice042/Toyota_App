<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns="sap.m">
	<CustomListItem class="carList2" type="Active" press="onCatalogPress">
		<VBox alignItems="Center" class="carBox3 sapUiTinyMarginBottom" renderType="Bare"
			visible="{path: 'oDataGlobalModel>isSelected', formatter:'inc.demo.Toyota.util.formatter.fnCarSelected'}">
			<VBox class="carImgBox sapUiSmallMarginTop">
				<Image src="{oDataGlobalModel>src}"/>
			</VBox>
			<Label text="{oDataGlobalModel>catalog}" class="sapUiTinyMarginTop"/>
		</VBox>
	</CustomListItem>
</core:FragmentDefinition>