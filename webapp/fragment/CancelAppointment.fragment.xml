<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
	<Dialog title="Information" draggable="true" contentWidth="30rem" resizable="true" class="sapUiSizeCompact warningDialogClass">
		<content>
			<VBox class="sapUiMarginTop" alignContent="Center" justifyContent="Center">
				<!--<content>-->
				<HBox class="sapUiSmallMarginTop" alignContent="Center" justifyContent="Center">
					<Text text="Your Appointment Cancelled Successfully " class="sapUiSmallMarginBegin sapUiTinyMarginTop"></Text>
				</HBox>
				<!--</content>-->
				<HBox class="sapUiSmallMarginBottom sapUiSmallMarginTop" alignContent="Center" justifyContent="Center">
					<Button width="4rem" text="Ok" press="onCancelAppointmentClose" class="cancelButton sapUiTinyMarginEnd"></Button>
				</HBox>
			</VBox>
		</content>
	</Dialog>
</core:FragmentDefinition>