<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
	<Dialog title="Information" draggable="true" contentWidth="30rem" resizable="true" class="sapUiSizeCompact warningDialogClass">
		<content>
			<Text text="Please Login to access this content" class="sapUiSmallMarginBegin sapUiTinyMarginTop"></Text>
		</content>
		<Toolbar class="sapUiTinyMarginTop sapUiTinyMarginBottom dialogToolbarClass">
			<Button text="Cancel" press="onCancel" class="dialogCancelBtn"></Button>
			<ToolbarSpacer></ToolbarSpacer>
			<Button text="Login" press="onContinue" class="dialogCancelBtn"></Button>
		</Toolbar>
	</Dialog>
</core:FragmentDefinition>